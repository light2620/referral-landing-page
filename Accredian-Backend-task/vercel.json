{
    "version": 2,
    "name": "referral-landing-page",
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "build": {
      "env": {
        "DATABASE_URL": "@database_url"
      }
    }
  }
  